{% extends 'commons/base.html.twig' %}

{% block body %}

<div class="tableGenre">
    <table>
        <thead>
            <tr>
                <th>Liste des genres</th>
            </tr>
        </thead>
        <tbody>
            {% for genre in genres %}
                <tr>
                    <td>{{ genre.name }}</td>
                </tr>
            {% endfor %}
        </tbody>
    </table>
</div>

{% endblock %}
